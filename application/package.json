{
  "name": "application",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "tailwind:dev": "pnpm tailwindcss -i ./src/content-script/tailwind.css -o ./src/content-script/tailwind.out.css --watch",
    "tailwind:build": "pnpm tailwindcss -i ./src/content-script/tailwind.css -o ./src/content-script/tailwind.out.css --minify",
    "dev": "pnpm concurrently \"pnpm tailwind:dev\" \"pnpm vite\"",
    "dev:demo": "vite --config ./src/web/vite.demo.cofnig.ts",
    "type-check": "tsc --noEmit",
    "build": "pnpm tailwind:build && tsc -b && vite build",
    "lint": "eslint . --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@shared/util": "workspace:*",
    "@xenova/transformers": "^2.17.2",
    "clsx": "^2.1.1",
    "lucide-react": "^0.474.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@config/eslint-config": "workspace:*",
    "@config/typescript": "workspace:*",
    "@types/chrome": "^0.0.304",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "chrome-webstore-upload-cli": "^3.3.1",
    "concurrently": "^9.1.2",
    "eslint": "^9.19.0",
    "postcss": "^8.5.1",
    "tailwindcss": "3.4.17",
    "typescript": "~5.7.2",
    "vite": "^6.1.0",
    "vite-plugin-string": "^1.2.3",
    "vite-plugin-web-extension": "^4.4.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.5"
  }
}
